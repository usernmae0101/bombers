{
    "name": "@bombers/game-server",
    "version": "0.1.0",
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev:build": "webpack --mode=development --watch",
        "dev:serve": "cross-env NODE_ENV=development nodemon dist/server.js",
        "prod": "npm-run-all --parallel prod:*",
        "prod:build": "webpack --mode=production",
        "prod:serve": "node dist/server.js"
    },
    "dependencies": {
        "@geckos.io/server": "^2.1.8",
        "@types/node": "^18.11.19",
        "array-buffer-serializer": "^0.4.0",
        "cross-env": "^7.0.3",
        "dotenv": "^9.0.0",
        "node-loader": "^2.0.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "socket.io": "^4.1.2",
        "socket.io-client": "^4.1.2",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2"
    }
}
